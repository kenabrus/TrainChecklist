@using System.Collections.Generic
@using System.Linq
@using Microsoft.AspNetCore.Http
@using TrainChecklist.ViewModels
@model LoginViewModel


    <div class="page-header header-filter" data-parallax="true" style="background-image: url('/img/hu_tango_ostrava_2018_04_21_dyna')">
        <div class="container">
            <!-- <div class="row">
                <div class="col-md-8 ml-auto mr-auto">
